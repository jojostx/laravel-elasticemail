{
    "name": "jojostx/laravel-elasticemail",
    "description": "A lightweight Laravel package for validating emails using the ElasticEmail  API.",
    "type": "library",
    "homepage": "https://github.com/jojostx/laravel-elasticemail",
    "license": "MIT",
    "authors": [
        {
            "name": "Ari Jay",
            "email": "ikuskid7@gmail.com"
        }
    ],
    "keywords": [
        "jojostx",
        "laravel",
        "laravel-package",
        "elasticemail",
        "email",
        "validation"
    ],
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/container": "^8.0|^9.0",
        "guzzlehttp/guzzle": "^6.3|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.2",
        "nunomaduro/larastan": "^0.6.11"
    },
    "autoload": {
        "psr-4": {
            "Jojostx\\ElasticEmail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jojostx\\ElasticEmail\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jojostx\\ElasticEmail\\Providers\\ElasticEmailProvider"
            ],
            "aliases": {
                "ElasticEmail": "Jojostx\\ElasticEmail\\Facades\\ElasticEmail"
            }
        }
    }
}